package multiplicatontable;

public class eveandodd {
	    public static void main(String[] args) {
	        
	        int even = 0;
	        int odd= 0;
	        for (int i = 0; i <= 20; i++) {
	            if (i % 2 == 0) {
	                even += i;  
	            } else {
	                odd += i;  
	            }
	        }
	        System.out.println( even);
	        System.out.println(odd);
	    }
	


}
